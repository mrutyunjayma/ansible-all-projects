plugin: amazon.aws.aws_ec2
strict: False
regions:
  - ap-south-1

filters:
  tag:Name: apache_SERVER

keyed_groups:
  - key: tags.Name
    prefix: tag_Name_

hostnames:
  - network_interfaces[0].association.public_ip # ✅ No quotes

compose:
  ansible_host: network_interfaces[0].association.public_ip # ✅ No quotes

vars:
  ansible_user: ubuntu
  ansible_ssh_private_key_file: /home/mj/.ssh/mj-keypair.pem

